<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.shuangyulin.domain">

	<class name="StudentSelectCourseInfo" table="t_StudentSelectCourseInfo">

          <!-- 主键记录编号 -->
          <id name="selectId" column="selectId">
            <generator class="native"/>
          </id>

          <!-- 多对一属性: 学生对象 -->
          <many-to-one name="studentNumber"
            column="studentNumber"
            class="Student_QQ287307421"
            lazy="false"
            outer-join="true"
            cascade="save-update"
          />

          <!-- 多对一属性: 课程对象 -->
          <many-to-one name="courseNumber"
            column="courseNumber"
            class="CourseInfo_QQ254540457"
            lazy="false"
            outer-join="true"
            cascade="save-update"
          />


		 
		
	</class>
	
</hibernate-mapping>
 
